{
  "compilerOptions": {
    "checkJs": false,
    "allowJs": true,
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "uploads",
    "exports"
  ]
}